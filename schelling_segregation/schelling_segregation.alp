					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[// Get neighbours of the same agent type
List<Agent> agents = findAll(getN(), 
	n->n.getAgentTypeId() == this.getAgentTypeId());

int sameTypeCount = 0; // Counts SchellingHousehold objects of the same type
for (Agent agent : agents) {
	if (agent instanceof SchellingHousehold) {
		SchellingHousehold household = (SchellingHousehold)agent;
		if (household.type == this.type) {
			sameTypeCount++;
		}
	}
}

return sameTypeCount;]]></Body>
